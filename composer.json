{
    "name": "nullref/yii2-geo",
    "description": "GEO Module for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","geo"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Yaroslav Velychko",
            "email": "lijkbezorger@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": ">=2.0.4",
        "nullref/yii2-core": "dev-master",
        "nullref/yii2-useful": "dev-master",
        "kartik-v/yii2-widget-depdrop": "@dev"
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://asset-packagist.org"
      }
    ],
    "config": {
      "process-timeout": 1800,
      "sort-packages": true,
      "fxp-asset": {
        "enabled": false
      }
    },
    "autoload": {
        "psr-4": {
            "nullref\\geo\\": "src/"
        }
    }
}
